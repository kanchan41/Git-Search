<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.kanchan.GitSearch">

    <uses-permission android:name="android.permission.INTERNET" />
    <uses-permission android:name="android.permission.ACCESS_WIFI_STATE" />

    <application
        android:allowBackup="true"
        android:icon="@drawable/mapprr"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".ScreenThree.ContributorRepository"></activity>
        <activity
            android:name=".ScreenFirst.MainActivity"
            android:windowSoftInputMode="adjustPan">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".ScreenTwo.RepositoryDetail" />
        <activity android:name=".ScreenTwo.ProjectLink" />
        <activity android:name=".ScreenThree.ProjectLink2" />
        <activity android:name=".ScreenThree.Repository2Detail"></activity>

    </application>

</manifest>